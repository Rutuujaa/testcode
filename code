import java.util.Scanner;

public class ClockWalk {
    
    public static void main(String[] args){
        Scanner scanner = new Scanner(System.in);
        
        System.out.print("Enter the time taken by A to cover 5 cmins (in minutes): ");
        int x = scanner.nextInt();
        
        System.out.print("Enter the time taken by B to cover 5 cmins (in minutes): ");
        int y = scanner.nextInt();
        
        double speedA = 5.0 / x;
        double speedB = 5.0 / y;
        
        int totalDistance = 60;
        
        double relativeSpeed = speedA + speedB;
        
        double timeToMeet = totalDistance / relativeSpeed;
        
        double distanceA = speedA * timeToMeet;
        
        System.out.printf("They will meet at %2f clock-minutes from 12-0-clock.\n", distanceA);
        
        scanner.close();
        
    }
}
